    "normalize.css": "^8.0.1",
    "prop-types": "15.7.2",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^6.15.5",
    "react-images-uploading": "^3.1.6",
    "react-lottie": "^1.2.3",
