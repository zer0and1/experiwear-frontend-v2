    "pm2": "^4.5.1",
    "prettier": "^2.4.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,json,css}": "prettier --single-quote --write"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "pre-commit": "lint-staged && git add .",
    "lint": "eslint src --fix",
    "format": "prettier --single-quote --write \"src/**/*.{js,jsx,json,css}\"",
    "=====PM2=====": "",
    "pm2:restart:dev": "yarn build && pm2 delete frontend || : &&  pm2 start frontend.json --env development",
    "pm2:restart:prod": "CONFIG_ENV=production yarn build && pm2 delete frontend || : &&  pm2 start frontend.json --env production",
    "deploy:dev": "pm2 deploy frontend.json dev update",
    "deploy:prod": "pm2 deploy frontend.json prod update"
  },
  "proxy": "https://hawks-dev.experiwear.com/api"
}
